            object cbSuperUserLine: TCheckBox
              Left = 7
              Height = 26
              Hint = 'Checking this indicates that this Command'#10'LIne will be run as ROOT. This uses the'#10'Linux program that your system has '#10'(kdesudo, gksudo, etc. to ensure it is '#10'secure).'#10#10'It uses a special prefix to indicate this,'#10'don''t change that.'#10#10'AS ALWAYS be circumspect in the use of '#10'SuperUser, it isquite powerful and at'#10'times quite dangerous. Know what you are'#10'doing.'#10#10'<end>'
              Top = 97
              Width = 76
              Caption = 'NT SU'
              Font.Height = -13
              OnChange = cbSuperUserLineChange
              OnKeyDown = memNotesLineKeyDown
              ParentFont = False
              TabOrder = 1
            end
            object cbIsFavoriteLine: TCheckBox
              Left = 7
              Height = 26
              Hint = 'Checking this box marks the Command Line'#10'as a "favorite" and it will show up in the '#10'Favorites list.'#10#10'It will be listed by the specified Command '#10'Line...'#10#10'...or you can give it a "friendly name" that '#10'is easier to read in the list.'#10#10'If you give it a friendly name the entry will'#10'be indented a couple of spaces and '#10'followed by the Command line for '#10'reference.'#10#10'<end>'
              Top = 128
              Width = 85
              Caption = 'NT FAV'
              Font.Height = -13
              OnChange = cbSuperUserLineChange
              OnKeyDown = memNotesLineKeyDown
              ParentFont = False
              TabOrder = 2
            end
            object cbDetachProcessLine: TCheckBox
              Left = 7
              Height = 26
              Hint = 'Checking this indicates that this Command'#10'LIne will be run as a DETACHED PROCESS.'#10#10'Detached processes run separately and'#10'independently from this program (that is,'#10'you can launch programs from here like'#10'gimp or firefox or whatever, with flags). '#10#10'Detached processes live only while this'#10'program lives. When this program closes'#10'any detahced processes it "owns" are'#10'also closed.'#10#10'You can manage Detached Processes on'#10'the "Proc''s" tab.'#10#10'<end>'
              Top = 161
              Width = 77
              Caption = 'NT DP'
              Font.Height = -13
              OnChange = cbSuperUserLineChange
              OnKeyDown = memNotesLineKeyDown
              ParentFont = False
              TabOrder = 3
            end
            object lblcapEntryLine: TLabel
