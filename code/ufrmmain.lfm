            object cbSuperUserLine: TCheckBox
              Left = 7
              Height = 21
              Hint = 'Checking this indicates that this Command'#10'LIne will be run as ROOT. This uses the'#10'Linux program that your system has '#10'(kdesudo, gksudo, etc. to ensure it is '#10'secure).'#10#10'It uses a special prefix to indicate this,'#10'don''t change that.'#10#10'AS ALWAYS be circumspect in the use of '#10'SuperUser, it isquite powerful and at'#10'times quite dangerous. Know what you are'#10'doing.'#10#10'<end>'
              Top = 77
              Width = 59
              Caption = 'NT SU'
              Font.Height = -12
              OnChange = cbSuperUserLineChange
              OnKeyDown = memNotesLineKeyDown
              ParentFont = False
              TabOrder = 5
